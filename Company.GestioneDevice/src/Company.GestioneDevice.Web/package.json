{
	"version": "1.0.0",
	"name": "my-app",
	"private": true,
	"scripts": {
		"dev": "npm run development",
		"development": "mix",
		"watch": "mix watch",
		"hot": "mix watch --hot",
		"prod": "npm run production",
		"production": "mix --production",
		"prettier": "npx prettier --check  **/Scripts/**/*.js  **/Vue/**/*.vue **/Styles/**/*.scss ",
		"prettier:fix": "npx prettier --write --ignore-unknown  **/Scripts/**/*.js  **/Vue/**/*.vue **/Styles/**/*.scss ",
		"prettier:watch": "onchange **/Scripts/**/*.js  **/Vue/**/*.vue **/Styles/**/*.scss -- prettier --write --ignore-unknown {{changed}}"
	},
	"dependencies": {
		"@abp/aspnetcore.mvc.ui.theme.leptonxlite": "~2.3.2",
		"pinia-plugin-persistedstate": "^3.1.2"
	},
	"devDependencies": {
		"@mdi/font": "^6.4.95",
		"axios": "^1.6.5",
		"css-loader": "^6.8.1",
		"laravel-mix": "^6.0.49",
		"lodash.debounce": "^4.0.8",
		"onchange": "^7.1.0",
		"pinia": "^2.1.7",
		"pinia-plugin-persistedstate": "^3.2.1",
		"prettier": "^3.2.2",
		"resolve-url-loader": "^5.0.0",
		"sass": "^1.69.7",
		"sass-loader": "^14.0.0",
		"vue": "^3.4.14",
		"vue-loader": "^17.4.2",
		"vuetify": "^3.5.2"
	}
}
